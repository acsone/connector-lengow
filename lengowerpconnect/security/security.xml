<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data noupdate="1">

        <!--  Lengow CATEGORY -->
        <record model="ir.module.category" id="module_category_lengow">
            <field name="name">Lengow</field>
            <field name="description">Lengow Connector Management</field>
            <field name="sequence">10</field>
        </record>

        <record model="res.groups" id="group_lengow_user">
            <field name="name">Lengow User</field>
            <field name="category_id" ref="lengowerpconnect.module_category_lengow"/>
        </record>

        
         <record model="res.groups" id="group_lengow_manager">
            <field name="name">Lengow Manager</field>
            <field name="category_id" ref="lengowerpconnect.module_category_lengow"/>
            <field name="implied_ids" eval="[
                (4, ref('connector.group_connector_manager')),
                (4, ref('group_lengow_user'))
            ]" />
        </record>

    </data>
</openerp>